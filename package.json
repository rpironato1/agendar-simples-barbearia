  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 8050",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "lint": "eslint .",
    "preview": "vite preview",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "check:all": "npm-run-all -p check:*",
    "check:lint": "eslint . && prettier --check src --ignore-unknown",
    "check:types": "tsc --noEmit",
    "check:test": "vitest --coverage",
    "check:e2e": "playwright test",
    "check:security": "npm audit && snyk test",
    "check:a11y": "pa11y-ci",
    "check:bundle": "size-limit",
    "check:perf": "lighthouse http://localhost:8050",
    "ci": "npm run check:all",
    "format": "prettier --write .",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch",
    "setup:graph": "./setup-dev-graph.sh",
    "dev:mcp": "node src/mcp/server.ts --port 8051",
    "dev:full": "concurrently \"npm run dev\" \"npm run dev:mcp\"",
    "analyze:graph": "madge --image dependency-graph.svg src/ --extensions ts,tsx",
    "analyze:circular": "madge --circular src/ --extensions ts,tsx",
    "analyze:complexity": "madge --summary src/ --extensions ts,tsx",
    "debug:level1": "echo 'üîç Testing Core & Integrations (Level 1)...'",
    "debug:level2": "npm run debug:level1 && echo 'üîç Testing Lib & Utils (Level 2)...'",
    "debug:level3": "npm run debug:level2 && echo 'üîç Testing Hooks & MCP (Level 3)...'",
    "debug:level4": "npm run debug:level3 && echo 'üîç Testing Components & API (Level 4)...'",
    "debug:level5": "npm run debug:level4 && echo 'üîç Testing Pages (Level 5)...'",
    "debug:full": "npm run debug:level5 && npm run test:e2e",
    "health:check": "curl -f http://localhost:8050/api/keep-alive || echo 'API not running'",
    "health:mcp": "curl -f http://localhost:8051/status || echo 'MCP not running'",
    "start:production": "npm run build && npm run preview",
    "dependencies:check": "npm outdated",
    "dependencies:update": "npm update",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
